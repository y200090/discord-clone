{
    "version": 2,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": Set-Cookie",
                    "value: "cookie-name=cookie-value; Path=/; HttpOnly; Secure; SameSite=None"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
