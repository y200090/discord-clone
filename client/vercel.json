{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "Set-Cookie": "*",
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Origin": "https://pprno-eldoccia.vercel.app"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}